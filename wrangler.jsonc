{
  "$schema": "node_modules/wrangler/config-schema.json",
  "name": "gravatar",
  "compatibility_date": "2024-04-01",
  "main": "./src/index.tsx",
  "assets": {
    "directory": "./dist"
  },
  "observability": {
    "enabled": true,
    "head_sampling_rate": 1
  },
  "routes": [
    {
      "pattern": "gravatar.zla.app",
      "custom_domain": true
    }
  ]
}
